  <div class="services-grid">
      {% for block in section.blocks %}
        <div class="service-card color-{{ block.settings.color_scheme }}">
          <div class="service-card-inner">
            <h3 class="service-title">{{ block.settings.title }}</h3>
            {% if block.settings.subtitle != blank %}
              <div class="service-subtitle">{{ block.settings.subtitle }}</div>
            {% endif %}
            {% if block.settings.text != blank %}
              <div class="service-text rte">{{ block.settings.text }}</div>
            {% endif %}

            {% if block.settings.image != blank %}
              <div class="service-media">
                <img src="{{ block.settings.image | img_url: '480x' }}" alt="{{ block.settings.image_alt }}" />
              </div>
            {% else %}
              <div class="service-media service-placeholder" aria-hidden="true">
                <img src="{{ 'service-placeholder.svg' | asset_url }}" alt="{{ block.settings.title }}" />
              </div>
            {% endif %}
          </div>
        </div>
      {% endfor %}
    </div>

